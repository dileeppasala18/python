#even_or_odd
def even_odd(num):
    if num%2==0:
        print(num," is even")
    else:
        print(num," is odd")
num=int(input("enter the number"))
even_odd(num)

#check_prime
def prime_check(x):
    c=0
    for i in range(1,x+1):
        z=x%i
        if z==0:
            c+=1
    if c==2:
        print("prime")
    else:
        print("not prime")
x=int(input("enter the number")       
prime_check(x)

#factorial_
def fact(x):
    result=1
    while x>=1:
        result=result*x
        x=x-1
    return result
x=int(input("enter the number"))
result=fact(x)
print(x,"factorial is",fact(x))
