#to print the multiplication table for a give number

n=int(input("enter the number"))
i=1
while(i<10):
    print(n,"*",i,"=",n*i)
    i+=1
print("end")






# to print the multiplication tables upto given number

n=int(input("enter the number"))
i=1
while(i<=n):
    j=1
    while(j<=10):
        print(i,"*",j,"=",i*j) 
        j+=1
    i+=1
print("bye")

